{
	"name": "Remo Music Player",
	"description": "The first local player in WEB",
	"version": "0.2.0.3",
	"manifest_version": 2,
	"offline_enabled": true,
	"permissions": [
		"fileSystem",
		"fullscreen",
		"notifications",
		"storage",
		"unlimitedStorage"
	],
	"app": {
		"launch": {
			"web_url": "http://remomusic.herokuapp.com/",
			"container": "panel",
			"width": 900,
			"height": 500
		}
	},
	"icons": {
		"16": "masterPlayer/images/app/16x16.png",
		"32": "masterPlayer/images/app/32x32.png",
		"64": "masterPlayer/images/app/64x64.png",
		"128": "masterPlayer/images/app/128x128.png",
		"256": "masterPlayer/images/app/256x256.png"
	}
}